# navigation
alias ..='cd ..'
alias ...='cd ../..'

# https://github.com/eza-community/eza
alias .='eza -lF --git --icons --group-directories-first --time-style relative'
# all detail
alias .a='eza -AlF --icons --time-style long-iso'
# tree
alias .t='eza -lF -T --level=3 --icons --time-style relative'

alias cp='cp -v'
alias ln='ln -v'
alias mv='mv -v'
alias rm='rm -v'
